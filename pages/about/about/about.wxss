form-group .title {
  min-width: calc(4em + 15px);
}
.cu-load-n.load-modal-n {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 140rpx;
  left: 0;
  margin: auto;
  width: 260rpx;
  height: 260rpx;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0 0 0rpx 2000rpx rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  font-size: 28rpx;
  z-index: 9999;
  line-height: 2.4em;
}

.cu-load-n.load-modal-n [class*="icon"] {
  font-size: 60rpx;
}

.cu-load-n.load-modal-n image {
  width: 70rpx;
  height: 70rpx;
}

.cu-load-n.load-modal-n::after {
  content: "";
  position: absolute;
  background: #fff;
  border-radius: 50%;
  width: 200rpx;
  height: 200rpx;
  font-size: 1px;
  border-top: 6rpx solid #f37b1d;
  border-right: 6rpx solid #f37b1d;
  border-bottom: 6rpx solid #f37b1d;
  border-left: 6rpx solid #f37b1d;
  animation: icon-spin 1s infinite linear;
  z-index: -1;
}
.item_container {
  display: flex;
  direction: row;
  width: 30rpx;
  height: 60rpx;
}
checkbox .wx-checkbox-input {
  width: 50rpx;
  height: 50rpx;
}